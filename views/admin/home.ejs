

<% include("../layouts/header.ejs") %>



<h1>Welcome <%= admin.name %></h1>

<button style="background-color: blue; color: aliceblue;"><a style="text-decoration: none; color: aliceblue;" href="/admin/dashboard">Dashboard</a></button>

<table style="width:50%">

  <tr>
    <td><img src="/userImages/<%= admin.image %>" alt="<%= admin.image %>" style="height: 100px; width: 100px;"></td>
  </tr>

  <tr>
    <td>Name</td>
    <td><%= admin.name %></td>
  </tr>

  <tr>
    <td>Email</td>
    <td><%= admin.email %></td>
  </tr>

  <tr>
    <td>Mobile</td>
    <td><%= admin.mobile %></td>
  </tr>
 
  
</table>

<button style="background-color: red; color: aliceblue;"><a style="text-decoration: none;" href="/admin/logout">Logout</a></button>



<% include("../layouts/footer.ejs") %>
