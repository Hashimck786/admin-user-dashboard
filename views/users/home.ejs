<% include("../layouts/header.ejs") %>



<h1>Welcome <%= user.name %></h1>

<table style="width:50%">

  <tr>
    <td><img src="userImages/<%= user.image %>" alt="<%= user.image %>" style="height: 100px; width: 100px;"></td>
  </tr>

  <tr>
    <td>Name</td>
    <td><%= user.name %></td>
  </tr>

  <tr>
    <td>Email</td>
    <td><%= user.email %></td>
  </tr>

  <tr>
    <td>Mobile</td>
    <td><%= user.mobile %></td>
  </tr>
 
  
</table>

<button style="background-color: red; color: aliceblue;"><a style="text-decoration: none;" href="/logout">Logout</a></button>



<% include("../layouts/footer.ejs") %>
